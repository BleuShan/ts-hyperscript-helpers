{
  "name": "ts-hyperscript-helpers",
  "version": "0.0.0-development",
  "description": "A typescript port of HyperScript-helpers",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "jsnext:main": "dist/es/index.js",
  "files": [
    "dist",
    "src"
  ],
  "repository": "http://github.com/bleushan/ts-hyperscript-helpers",
  "bugs": "http://github.com/bleushan/ts-hyperscript-helpers/issues",
  "keywords": [
    "HyperScript"
  ],
  "homepage": "http://github.com/bleushan/ts-hyperscript-helpers",
  "author": "Philippe Lafontaine <lafontaine.philippe@gmail.com>",
  "contributors": [
    "Bleushan"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs": "tsc --outDir dist/cjs",
    "build:es": "tsc --target es6 --outDir dist/es",
    "build": "yarn run clean && yarn run build:cjs && yarn run build:es",
    "test": "yarn run build && mocha --ui bdd --recursive dist/cjs/test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "3.5.2",
    "@types/hyperscript": "0.0.2",
    "@types/mocha": "2.2.41",
    "@types/ramda": "github:types/npm-ramda",
    "chai": "3.5.0",
    "cz-conventional-changelog-emoji": "0.1.0",
    "hyperscript": "2.0.2",
    "jsdom": "11.0.0",
    "jsverify": "github:jsverify/jsverify",
    "mocha": "3.4.2",
    "rimraf": "2.6.1",
    "semantic-release": "^6.3.6",
    "tslint": "5.3.2",
    "tslint-eslint-rules": "4.1.0",
    "typescript": "2.3.3"
  },
  "dependencies": {
    "ramda": "0.23.0"
  },
  "peerDependencies": {
    "tslib": "1.7.1"
  },
  "engines": {
    "node": "^7.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  }
}
