{
  "name": "ts-hyperscript-helper",
  "version": "1.0.0",
  "description": "A typescript port of HyperScript-helpers",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "jsnext:main": "dist/es/index.js",
  "files": [
    "dist",
    "src"
  ],
  "repository": "http://github.com/bleushan/ts-hypescript-helpers",
  "bugs": "http://github.com/bleushan/ts-hypescript-helpers/issues",
  "keywords": [
    "HyperScript"
  ],
  "homepage": "http://github.com/bleushan/ts-hypescript-helpers",
  "author": "Philippe Lafontaine <lafontaine.philippe@gmail.com>",
  "contributors": [
    "Bleushan"
  ],
  "scripts": {
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:es": "tsc --outDir dist/es",
    "build": "yarn run build:cjs && yarn run build:es"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/ramda": "github:types/npm-ramda",
    "cz-conventional-changelog-emoji": "0.1.0",
    "tslint": "5.3.2",
    "tslint-eslint-rules": "4.1.0",
    "typescript": "2.3.3"
  },
  "dependencies": {
    "ramda": "0.23.0",
    "tslib": "1.7.1"
  },
  "engines": {
    "node": "^7.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  }
}
